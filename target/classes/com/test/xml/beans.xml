<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
   <bean id="world" class="com.test.beans.World">
	   <property name="wave" value= "Hello world"></property>
	    </bean>

   <bean id = "human" class = "com.test.beans.Human">
	   <property name = "country" ref = "country">
		    <bean id = "country" class = "com.test.beans.Country">
	   			<property name ="name" value = "Canada"></property>
	  		    <property name = "city">
		   			<bean id = "city" class = "con.test.beans.City">
			 		  <property name = "name" value = "Vancouver"></property>
		  			</bean>
	   			</property>
  			</bean>
	   </property>
	   <property name = "id" value = "1"></property>
	   <property name = "name" value = "James"></property>
	   <property name = "nickname" value = "James97"></property>
   </bean>	   
  
   
</beans>